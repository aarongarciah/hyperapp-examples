{
  "connection": "default",
  "collectionName": "project",
  "info": {
    "name": "project",
    "description": "A submitted Hyperapp Project"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "author": {
      "type": "string",
      "required": true
    },
    "thumbnail": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "description": {
      "type": "text"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "published",
        "trashed"
      ]
    },
    "link": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "github": {
      "type": "string"
    }
  }
}